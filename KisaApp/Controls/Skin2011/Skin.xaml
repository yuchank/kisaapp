<Classes:InstrumentClusterSkin 
      x:Class="KisaApp.Controls.Skin2011.Skin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:Classes="clr-namespace:KisaApp.Classes"
      xmlns:local="clr-namespace:KisaApp.Controls.Skin2011"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1280">

    <Grid x:Name="LayoutRoot">
    <Image Source="/KisaApp;component/Resources/2011/2011-Background.png" Stretch="Fill" />
    <Canvas HorizontalAlignment="Center" VerticalAlignment="Center">
      <Slider x:Name="uxRPM" Template="{DynamicResource SliderControlTemplate_2011LargeNeedle}" Value="{Binding Rpm}"  Maximum="8000" Canvas.Left="304" Canvas.Top="-16"/>
      <Slider x:Name="uxMPH" Template="{DynamicResource SliderControlTemplate_2011LargeNeedle}" Maximum="160" Canvas.Left="-322" Canvas.Top="-16"/>
    </Canvas>
    <Image Source="/KisaApp;component/Resources/2011/2011-Overlay.png" Stretch="Fill" IsHitTestVisible="False" d:IsLocked="True" />
  </Grid>
</Classes:InstrumentClusterSkin>
