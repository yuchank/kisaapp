<UserControl x:Class="KisaApp.SkinSelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KisaApp"
             xmlns:Skin2011="clr-namespace:KisaApp.Controls.Skin2011"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="OnLoaded" Unloaded="OnUnloaded">
  <Grid x:Name="LayoutRoot">
    <StackPanel Orientation="Horizontal">
      <Skin2011:Skin x:Name="uxSkin2011" />
    </StackPanel>
  </Grid>
</UserControl>
